{% macro content(spec) %}

{% set spec = spec or {} %}
{% set _class = spec.class or '' %}

<!-- portfolio -->
<div class="bloc l-bloc bgc-white tc-manatee" id="portfolio">
  <div class="container bloc-lg">
    <div class="row">
      <div class="col-sm-12">
        <h2 class="mg-md text-center tc-green-ryb ">
          <strong>Our Portfolio</strong>
        </h2>
        <div class="divider-h">
          <span class="divider divider-half"></span>
        </div>
        <p class="text-center">
          We invest in visionary founders
        </p>
      </div>
    </div>
  </div>
</div>
<!-- portfolio END -->

<!-- Bloc Group -->
<div class='bloc-group'>

<!-- bloc-5 -->
<div class="bloc l-bloc bgc-white bloc-tile-2 tc-manatee" id="bloc-5">
