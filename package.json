{
  "name": "inreach-pub",
  "version": "0.0.1",
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "bootstrap-tooltip": "^3.1.1",
    "iconfonts": "^0.7.0",
    "jquery": "~2.1.4",
    "normalize.css": "~3.0.3",
    "superagent": "latest"
  },
  "devDependencies": {
    "autoprefixer": "~6.0.3",
    "browser-sync": "~2.9.11",
    "browserify": "~12.0.0",
    "browserify-shim": "^3.8.11",
    "del": "~2.0.2",
    "envify": "~3.4.0",
    "foldero": "~0.1.1",
    "glob": "~5.0.15",
    "gulp": "~3.9.0",
    "gulp-changed": "~1.3.0",
    "gulp-data": "~1.2.0",
    "gulp-eslint": "~1.0.0",
    "gulp-gh-pages": "latest",
    "gulp-htmlmin": "~1.2.0",
    "gulp-if": "~2.0.0",
    "gulp-imagemin": "~2.3.0",
    "gulp-load-plugins": "~1.0.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-newer": "~1.0.0",
    "gulp-nunjucks-html": "~2.0.0",
    "gulp-plumber": "~1.0.1",
    "gulp-postcss": "~6.0.1",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~2.0.4",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.4.2",
    "gulp-util": "~3.0.7",
    "imagemin-pngquant": "~4.2.0",
    "imagemin-svgo": "~4.1.2",
    "lodash": "~3.10.1",
    "minimist": "~1.2.0",
    "nunjucks": "~2.1.0",
    "rimraf": "~2.4.3",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.5.0 ",
    "wrench": "~1.5.8"
  },
  "scripts": {
    "clean-deps": "rimraf node_modules"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "ammap3": "global:AmCharts"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}"
    }
  }
}
